{
  "name": "server",
  "version": "1.0.0",
  "description": "TypeScript boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "nodemon dist/app/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon app/index.ts | pino-pretty",
    "test": "mocha -r ts-node/register test/**/*.test.ts --exit",
    "lint": "eslint -c .eslintrc.js \"{app, test}/**/*.{js,ts,tsx}\" --quiet",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "lint:fix": "eslint -c .eslintrc.js \"{app, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" app/index.ts | pino-pretty"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "events": "^3.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.3",
    "inversify": "^5.0.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "pino": "^6.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-express-middleware": "^4.0.2",
    "tslog": "^3.0.1",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/cors": "^2.8.7",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "^4.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.10",
    "@types/node-cache": "^4.2.5",
    "@types/node-fetch": "^2.5.7",
    "@types/pino": "^6.3.0",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.2.0",
    "envfile": "^6.14.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.2.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.1.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.0",
    "typescript": "^4.0.2"
  }
